<!DOCTYPE html>
<html lang="en">
<head>
{% block head %}
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>{% block title %}Stephen F. Austin - Digital Collection{% endblock title %}</title>

  {% block metadata %}
    <meta property="og:site_name" content="Digital Austin Papers">
    {% block metadata_opengraph %}
    <meta property="og:type" content="website">
    <meta property="og:url" content="http://digitalaustinpapers.org">
    <meta property="og:title" content="Stephen F. Austin - Digital Collection">
    <meta property="og:description" content="Stephen F. Austin's letters">
    {% endblock metadata_opengraph %}
    <meta property="twitter:card" content="summary">
  {% endblock metadata %}

  <!-- external fonts -->
  {% block fonts %}
    <link href="//fonts.googleapis.com/css?family=Stint+Ultra+Expanded:400" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Stint+Ultra+Condensed:400" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Quicksand:300,400,700" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Bitter:400,400italic,700" rel="stylesheet">
  {% endblock fonts %}

  <!-- CSS -->
  {% block stylesheets %}
    <link rel="stylesheet" href="assets/css/style.css">
  {% endblock stylesheets%}

  <!-- scripts -->
  {% block scripts %}
  <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
  <script src="assets/js/html5shiv/html5shiv-3.7.2.min.js"></script>
  <script src="assets/js/respond/respond-1.4.2.min.js"></script>
  <![endif]-->

  <!-- Google Analytics -->
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-66221617-1', 'auto');
    ga('require', 'linkid', 'linkid.js');
    ga('send', 'pageview');

  </script>
  {% endblock scripts %}

{% endblock head %}
</head>
<body>
<div id="main-wrapper">
  {% block header %}
    {% include 'header.twig' %}
  {% endblock header %}

  {% block hero %}
  {% endblock hero %}

  <div class="container main-container">
    <div class="row">
      {% block main_container %}
      <div class="col-md-12">
        {% block content %}{% endblock content %}
      </div><!-- col-sm-12 -->
      {% endblock main_container%}
    </div><!-- row -->
  </div><!-- main-container -->

</div><!-- main-wrapper -->
  {% block footer %}
    {% include 'footer.twig' %}
  {% endblock footer %}

<!-- scripts -->
{% block scripts_footer %}
<script src="assets/js/jquery/jquery-1.9.1.min.js"></script>
<script src="assets/js/bootstrap/bootstrap-3.3.5.min.js"></script>
<script src="assets/js/d3/d3.min.js"></script>
{% endblock scripts_footer %}

</body>
</html>
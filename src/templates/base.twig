<!DOCTYPE html>
<html lang="en">
<head>
{% block head %}
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>{% block title %}Stephen F. Austin - Digital Collection{% endblock %}</title>


  <!-- CSS -->
  {% block stylesheets %}
    <link rel="stylesheet" href="assets/css/style.css">
    <link rel="stylesheet" href="assets/css/bootstrap-theme.min.css">
  {% endblock %}

  <!-- scripts -->
  {% block scripts %}
  <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
  <script src="assets/js/html5shiv/html5shiv-3.7.2.min.js"></script>
  <script src="assets/js/respond/respond-1.4.2.min.js"></script>
  <![endif]-->
  {% endblock %}
{% endblock %}
</head>
<body>
<div id="main-wrapper">
  {% block header %}
    {% include 'header.twig' %}
  {% endblock %}

  <div class="container main-container">
    <div class="row">
      <div class="col-sm-4">
        {% block sidebar %}{% endblock %}
      </div><!-- col-sm-4 -->
      <div class="col-sm-8">
        {% block content %}{% endblock %}
      </div><!-- col-sm-8 -->
    </div><!-- row -->
  </div><!-- main-container -->

</div><!-- main-wrapper -->
<div id="footer">
  {% block footer %}
    {% include 'footer.twig' %}
  {% endblock %}
</div>

<!-- scripts -->
{% block scripts_footer %}
<script src="assets/js/jquery/jquery-1.9.1.min.js"></script>
<script src="assets/js/jquery-ui/jquery-ui-1.10.3.min.js"></script>
<script src="assets/js/bootstrap/bootstrap-3.3.5.min.js"></script>
{% endblock %}

</body>
</html>